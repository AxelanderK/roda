<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field='messages' type='config.i18n.client.ClientMessages' />

	<g:FlowPanel addStyleNames="removable-textbox">
		<g:TextBox title="{messages.removableTextBox}" addStyleNames="form-textbox form-textbox-medium" ui:field="item">
		</g:TextBox>
		<g:Anchor addStyleNames="ri-toolbarLink ri-toolbarLink-bg bullet" ui:field="removeDynamicTextBoxButton" />
	</g:FlowPanel>
</ui:UiBinder>
